

<div>
    <% if current_user %>   
        <h4><%= current_user.email %></h4>
        <%= link_to "log out" ,  destroy_user_session_path  , method: :delete %>
    <% end %>
</div>
<div>
    <% turbo_stream_from 'rooms'%>
    <h1>Rooms</h1>
    <%= render @rooms %>
<% turbo_stream_from 'users'%>
    <h1>Users</h1>
    <%= render @users %>
</div>
